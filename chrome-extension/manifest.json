{
    "manifest_version": 3,
    "name": "LeetCode AI Mentor",
    "version": "1.0",
    "description": "An AI-powered mentor to help you solve LeetCode problems without giving away the solution.",
    "permissions": [
        "storage"
    ],
    "host_permissions": [
        "http://localhost:3000/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://leetcode.com/problems/*"
            ],
            "css": [
                "style.css"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}